{
    "name": "chesscom/chess-game",
    "description": "Represents a chess game as a php object",
    "keywords": ["chess", "php"],
    "homepage": "https://github.com/ChessCom/Chess-Game",
    "type": "library",
    "license": "PHP-3.01",
    "authors": [
        {
            "name": "Chess team",
            "homepage": "http://www.chess.com"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.12.0",
        "phpunit/phpunit": "~4.8.0",
        "jtreminio/test-extensions": "~1.0.0"
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Chess\\Game": "src/"
        },
        "files": ["PEAR.php"]
    }
}
